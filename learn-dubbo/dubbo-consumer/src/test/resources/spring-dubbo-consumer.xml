<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
          http://code.alibabatech.com/schema/dubbo
          http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

  <dubbo:application name="cheng-dubbo-consumer"/>

  <dubbo:registry id="zk1" address="115.29.47.207:2181" protocol="zookeeper"/> <!-- 注意要关闭防火墙 -->

  <dubbo:consumer registry="zk1"/>

  <dubbo:reference id="speakInterface" interface="com.cheng.dubbo.SpeakInterface"/>
  <dubbo:reference id="nameInterface" interface="com.cheng.dubbo.NameInterface"/>
  <dubbo:reference id="growUpInterface" interface="com.cheng.dubbo.GrowUpInterface"/>
  <!--<dubbo:reference id="sexInterface" interface="com.cheng.dubbo.SexInterface"/>-->
  <!--<dubbo:reference id="allInfoInterface" interface="com.cheng.dubbo.AllInfoInterface"/>-->
</beans>
